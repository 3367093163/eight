void TEST_MainPage(void)
{	
	GUI_ShowCHinese(28,20,16,"陈佳雪",1);//中文姓名
	GUI_ShowString(4,48,"632007030402",16,1);//数字详细
	delay_ms(1500);		
	delay_ms(1500);
}

int main(void)
{	
	delay_init();	    	       //延时函数初始化	  
	OLED_Init();			         //初始化OLED  
	OLED_Clear(0);             //清屏（全黑）
	while(1) 
	{	
		TEST_MainPage();         //界面显示
	}
}

